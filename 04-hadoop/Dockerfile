FROM squidsolutions/bouquet-war

ADD http://openbouquet.io/download/plugins/bouquet-plugin-apachedrill-4.2.3-jar-with-dependencies.jar /opt/squid/bouquet/plugins/bouquet-plugin-apachedrill-4.2.3-jar-with-dependencies.jar
ADD http://openbouquet.io/download/plugins/bouquet-plugin-spark-4.2.3-jar-with-dependencies.jar /opt/squid/bouquet/plugins/bouquet-plugin-spark-4.2.3-jar-with-dependencies.jar
RUN chmod +r /opt/squid/bouquet/plugins/*.jar
