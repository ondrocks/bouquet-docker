FROM squidsolutions/bouquet-war


ADD http://openbouquet.io/download/plugins/bouquet-plugin-postgresql-4.2.2-jar-with-dependencies.jar /opt/squid/bouquet/plugins/bouquet-plugin-postgresql-4.2.2-jar-with-dependencies.jar
ADD http://openbouquet.io/download/plugins/bouquet-plugin-mysql-4.2.2-jar-with-dependencies.jar /opt/squid/bouquet/plugins/bouquet-plugin-mysql-4.2.2-jar-with-dependencies.jar

RUN chmod +r /opt/squid/bouquet/plugins/*.jar

